                        <div class="row">
                            <div class="col-sm-12 col-lg-12 col-md-12">
                                <div class="card-box">

                                    <h4 class=" m-t-0 header-title text-center"><b>Pictures of Location</b></h4>
                                    <p class="text-muted m-b-30 font-13"></p>

                                    <% let imageArr=[] %>

                                        <% postDetail.postIDs.forEach((postID)=>{
                                            imageArr.push(...postID.image.imageLarge)
                                            }) %>




                                            <ul class="owl-carousel owl-theme">
                                                <%if( imageArr) { %>
                                                    <% imageArr.forEach((imageLarge)=>{ %>
                                                        <li><a class="item" id="carousel-image-sectio"
                                                                data-fancybox="gallery" href="<%=imageLarge%>"
                                                                data-caption="Caption1"><img class="imgsize"
                                                                    src="<%=imageLarge%>" alt="01"></a></li>
                                                        <%}) %>

                                                            <%} %>

                                            </ul>

                                </div>
                            </div><!-- col-lg-12 -->
                        </div><!-- End Row -->

                        <% if(locationReviews && locationReviews.length) {%>



                            <div class="row">
                                <div class="col-sm-12 col-lg-12 col-md-12">

                                    <div class="card-box">

                                        <div class="gtco-testimonials">
                                            <h2>Reviews</h2>
                                            <div class="owl-carousel owl-carousel1 owl-theme">
                                                <%locationReviews.forEach((reviews)=> { %>
                                                <div>
                                                    <div class="card text-center">
                                                        <%if(reviews.userId && reviews.userId.userBasicInfo && reviews.userId.userBasicInfo.profilePicture ){%>
                                                            <img class="card-img-top"  src="<%=reviews.userId.userBasicInfo.profilePicture%>" alt="">
                                                        <%} %>
                                                         
                                                       
                                                        <div class="card-body">
                                                            <h5><%= reviews.userId && reviews.userId && reviews.userId.userInfo && reviews.userId.userInfo.firstName ? reviews.userId.userInfo.firstName : `` %> <br />
                                                                <span> <%= reviews.userId && reviews.created_on ?  new Date(reviews.created_on).toLocaleDateString('en-US', { year: 'numeric', month: 'short', day: 'numeric' })  : `` %> </span>
                                                            </h5>
                                                            <p class="card-text">“<%= reviews.commentText%>” </p>
                                                        </div>
                                                    </div>
                                                </div>

                                                <%})%>
                                                
                                            </div>
                                        </div>




                                      

                                       


                                    </div>
                                </div>
                            </div>

                            <%}%>









                                <div class="row">
