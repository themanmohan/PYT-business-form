                <div class="col-lg-6">

                    <div class="card-box">
                        <h4 class="m-t-0 header-title"><b>Name and Address</b></h4>

                        <p class="text-muted font-13 m-b-30">
                            Fill the firm name and Address
                        </p>

                        <div class="form-group">
                            <label for="location_name">Location Name*</label>
                            <input type="text" value="<%= postDetail && postDetail.location.placeTag ? postDetail.location.placeTag : ``%>" name="location_name" parsley-trigger="change"  placeholder="Enter Location Name" class="form-control" id="location_name">
                        </div>

                        <div class="form-group">
                            <label for="city">City*</label>
                            <input type="text" name="city" value="<%= postDetail && postDetail.location.city ? postDetail.location.city : ``%>" parsley-trigger="change"  placeholder="Enter City" class="form-control" id="city">
                        </div>

                        <div class="form-group">
                            <label for="country">Country*</label>
                            <input type="text" value="<%= postDetail && postDetail.location.country ? postDetail.location.country : ``%>"  name="country" parsley-trigger="change"  placeholder="Enter Country" class="form-control" id="country">
                        </div>

                        <div class="form-group">
                            <label for="address">Address and Landmark*</label>
      
                            <textarea type="address"  name="address" parsley-trigger="change"  placeholder="Enter Address" class="form-control" id="address"> <%= postDetail ? postDetail.location.address : ``%> </textarea>
                        </div>
                    
                    </div>

                </div>

