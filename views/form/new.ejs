<%- include(`../partials/head`) %>

    <link href='https://fonts.googleapis.com/css?family=Poppins' rel='stylesheet'>
    <link rel="stylesheet" type="text/css" href="//cdn.jsdelivr.net/npm/slick-carousel@1.8.1/slick/slick.css" />
    <link rel="stylesheet" type="text/css" href="/styles/businessForm.css">

    <%- include(`../partials/head-end`) %>

        <%- include(`../partials/header`) %>

            <!--========================Body Content========================/---->
            <div class="wrapper">

                <div class="container">

                    <div class="row">

                        <div class="col-sm-12">

                            <h4 class="page-title">Fill Your Business Information</h4>

                            <ol class="breadcrumb ">
                                <li class="active">
                                    <%= docTitle%>
                                </li>
                                <li class="active">Forms</li>
                                <li class="active">Information</li>
                            </ol>

                        </div>

                    </div>


                    <form id="business-form">

                        <div class="row">

                            <div class="col-lg-6">

                                <div class="card-box">
                                    <h4 class="m-t-0 header-title"><b>Name and Address</b></h4>

                                    <p class="text-muted font-13 m-b-30">
                                        Fill the firm name and Address
                                    </p>

                                    <p id="country-code" data-id="<%=CountryISOCode%>"></p>

                                    <div class="form-group">
                                        <label for="location_name">Location Name*</label>
                                        <input type="text"
                                            value="<%= postDetail && postDetail.placeTag ? postDetail.placeTag : ``%>"
                                            name="location_name" parsley-trigger="change"
                                            placeholder="Enter Location Name" class="form-control" id="location_name">
                                    </div>

                                    <div class="form-group">
                                        <label for="city">City*</label>
                                        <input type="text" name="city"
                                            value="<%= postDetail && postDetail.city ? postDetail.city : ``%>"
                                            parsley-trigger="change" placeholder="Enter City" class="form-control"
                                            id="city">
                                    </div>

                                    <div class="form-group">
                                        <label for="country">Country*</label>
                                        <input type="text"
                                            value="<%= postDetail && postDetail.country ? postDetail.country : ``%>"
                                            name="country" parsley-trigger="change" placeholder="Enter Country"
                                            class="form-control" id="country">
                                    </div>

                                    <div class="form-group">
                                        <label for="address">Address and Landmark*</label>

                                        <textarea type="address" name="address" parsley-trigger="change"
                                            placeholder="Enter Address" class="form-control"
                                            id="address"><%= postDetail ? postDetail.postIDs[0].location.address : ``%> </textarea>
                                    </div>

                                </div>

                            </div>

                            <div class="col-lg-6">
                                <div class="card-box">
                                    <h4 class="m-t-0 header-title"><b>Basic Info</b></h4>
                                    <p class="text-muted font-13 m-b-30">
                                        We want some of your info to show in our app.
                                    </p>

                                    <div class="form-group">
                                        <label for="CityName">Interests*</label>
                                        <p class="text-muted  font-12">Select Category In which your firm lies</p>
                                        <select class="selectpicker m-b-0" name="interests" multiple
                                            data-max-options="3" data-style="btn-white">
                                            <option>Food & Wine</option>
                                            <option>City Life</option>
                                            <option>Nightlife & Entertainment</option>
                                            <option>History, Art & Culture</option>
                                            <option>Nature</option>
                                            <option>Mountains</option>
                                            <option>Beaches</option>
                                            <option>Lakes & Rivers</option>
                                            <option>Wildlife</option>
                                            <option>Dunes & Deserts</option>
                                            <option>Road Trips</option>
                                            <option>Cruises</option>
                                            <option>Adventure Sports</option>
                                            <option>Hotels & Wellness</option>
                                            <option>Retail Therapy</option>
                                            <option>Kid Friendly</option>
                                            <option>Other</option>
                                        </select>
                                    </div>



                                    <div class="form-group" id="contact-number-section">
                                        <label for="Country">Contact Number*</label>
                                        <input id="contact_number" name="contact_number" class="form-control"
                                            type="tel">
                                    </div>


                                    <div class="form-group">
                                        <label for="Website">Website*</label>
                                        <p class="text-muted  font-12">https://www.example.com</p>
                                        <input type="text" name="website" parsley-trigger="change"
                                            placeholder="Enter Website Link" class="form-control" id="website">
                                    </div>

                                </div>
                            </div>
                        </div><!-- end row -->

                        <div class="row">

                            <div class="col-lg-12">

                                <div class="card-box">
                                    <h4 class="m-t-0 header-title"><b>TIMINGS*</b></h4>

