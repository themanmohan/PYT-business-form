<body>


    <!--======================== Navigation Bar ========================/---->

    <header id="topnav">
        <div class="topbar-main">
            <div class="container">

                <!-- Logo container-->
                <div class="logo">
                    <a class="logo"><span>PYT - Business</span></a>
                </div>
                <!-- End Logo container-->


                <div class="menu-extras">

                    <ul class="nav navbar-nav navbar-right pull-right">



                        <li class="dropdown navbar-c-items">
                            <a href="/business-form/edit/<%=businessFormDetail && businessFormDetail._id ?  businessFormDetail._id: ``%>"
                                class="dropdown-toggle waves-effect waves-light profile"><i
                                    class="glyphicon glyphicon-edit "></i>&nbsp; Edit Info</a>
                        </li>
                    </ul>

                </div>

            </div>
        </div>
    </header>
    <!-- End Navigation Bar-->




    <!--========================Body Content========================/---->


    <div class="wrapper">
        <div class="container">


            <!--========== Basic Info ===================!-->

            <div class="row">
                <div class="col-sm-12 ">
                    <div class="card-box">
                        <div class="row">
                            <div class="col-sm-12">
                                <h4 class=" m-t-0 header-title"><b>Basic Info</b></h4>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-6">

                                <h2 class="m-0 text-dark  font-600"><%=businessFormDetail && businessFormDetail.location_name ? businessFormDetail.location_name: ``%></h2>
                                <p class="m-0 text-dark"><%=businessFormDetail && businessFormDetail.city ? businessFormDetail.city: ``%>, <%=businessFormDetail && businessFormDetail.country ? businessFormDetail.country: ``%></p>
                                <hr>

                                <button type="button" class="btn btn-primary" data-toggle="modal"
                                    data-target="#myModal">Click Here To view Timings</button>


                                <!-- Modal -->
                                <div class="modal fade" id="myModal" tabindex="-1" role="dialog"
                                    aria-labelledby="exampleModalLabel" aria-hidden="true">
                                    <div class="modal-dialog" role="document">
                                        <div class="modal-content">
                                            <div class="modal-header">
                                                <h5 class="modal-title" id="exampleModalLabel">Opening Timings</h5>

                                            </div>
                                            <div class="modal-body">
                                                <div class="panel" id="Business_hour_panel">

                                                    <div class="body">
                                                        <table class="table">
                                                            <tbody>
                                                                
                                                      
                                                                <%if(businessFormDetail && businessFormDetail.timing && businessFormDetail.timing.length ){%>

                                                                    <%businessFormDetail.timing.forEach((time)=>{%>
                                                                        <tr class="success">
                                                                            <td><%=time.day%></td>
                                                                            <td><%=time.open_timing%> to <%=time.close_timing%></td>
                                                                        </tr>
                                                                    <% }) %>

                                                                <% } else { %>
                                                                    <tr class="success">
                                                                        <td>Available</td>
                                                                        <td>24 Hours</td>
                                                                    </tr>
                                                                <% } %>
                                                               
                                                         
                                                            </tbody>
                                                        </table>
                                                    </div>

                                                </div>
                                            </div>
                                            <div class="modal-footer">
                                                <button type="button" class="btn btn-secondary"
                                                    data-dismiss="modal">Close</button>
                                            </div>
                                        </div>
                                    </div>
                                </div>


                                <hr>

                                <p class="m-0 text-dark">Location</p>
                                <h4>  <%=businessFormDetail && businessFormDetail.location_name ? businessFormDetail.location_name: ``%></h4>
                                <hr>

                                <p class="m-0 text-dark">Website</p>
                                <a href="#">
                                    <h4 style="color:#00b4ff;">
                                        <%=businessFormDetail && businessFormDetail.website ?
                                            businessFormDetail.website: ``%>
                                    </h4>
                                </a>
                                <hr>

                                <p class="m-0 text-dark">Interests</p>
                                <h4>
                                    <%= businessFormDetail && businessFormDetail.interests ? businessFormDetail.interests.map(interest=>interest): ``%>
                                </h4>
                                <hr>


                                <p class="m-0 text-dark">Address</p>
                                <h4>
                                    <%=businessFormDetail && businessFormDetail.address ? businessFormDetail.address:
                                        ``%>
                                </h4>
                                <hr>


                                <p class="m-0 text-dark">Contact Info</p>
                                <h4>
                                   Phone Number :- <%=businessFormDetail && businessFormDetail.contact_number ?
                                        businessFormDetail.contact_number: ``%>
                                </h4>
                                <h4>
                                   Email Address :- <%= businessFormDetail && businessFormDetail.email_address ?
                                        businessFormDetail.email_address: ``%>
                                </h4>


                            </div>
                            <!-- col-md-6 -->

                            <div class="col-md-6">

                                <div id="map"></div>

                            </div>
                            <!-- col-md-6 -->
                        </div>
                    </div>
                </div>
            </div><!-- End Row -->


            <!--========== Basic Info Ends Here ===================!-->





            <!--========== Description ===================!-->

            <div class="row">
                <div class="masonry-container  ">
                    <div class="col-sm-12 col-lg-12 col-md-12">
                        <div class="card-box">
                            <h3 class="header-title m-t-0 "><b>Description</b></h3>
                            <p class="text-muted m-b-20 font-13 ">
                                <%= businessFormDetail && businessFormDetail.description ? businessFormDetail.description : ``%>
                            </p>

                        </div>
                    </div><!-- col-lg-12 -->
                </div><!-- End Row -->

                <!--========== Description Ends Here ===================!-->





                <!--========== Location Pictures ===================!-->

                <div class="row" >
                    <div class="col-sm-12 col-lg-12 col-md-12">
                        <div class="card-box" id="image-card-section">

                            <h4 class=" m-t-0 header-title"><b>Pictures of Location<%=businessFormDetail.media.gallery.length%></b></h4>
                            <p class="text-muted m-b-30 font-13"></p>

                            <div class="owl-carousel owl-theme">

                                <%if( businessFormDetail && businessFormDetail.media) { %>
                                    <% businessFormDetail.media.gallery.forEach((image)=>{ %>
                                        <div class="item">
                                            <img style="height: 130px; border-radius: 5px;" src="<%=image.resource_url%>" />
                                        </div>
                                  
                                    <%}) %>

                                <%} %>

                            </div>
                        </div>
                    </div><!-- col-lg-12 -->
                </div><!-- End Row -->

                <!--========== Location Info Ends Here ===================!-->


                                <!--========== PYT Pictures ===================!-->
                                <%if( businessFormDetail && businessFormDetail.pytImages && businessFormDetail.pytImages.length ) { %>
                                <div class="row">
                                    <div class="col-sm-12 col-lg-12 col-md-12">
                                        <div class="card-box" id="image-card-section">
                
                                            <h4 class=" m-t-0 header-title"><b>More Gallery</b></h4>
                                            <p class="text-muted m-b-30 font-13"></p>
                
                                            <div class="owl-carousel owl-theme">
                
                                                
                                                    <% businessFormDetail.pytImages.forEach((imgLink)=>{ %>
                                                        <div class="item">
                                                            <img style="height: 130px; border-radius: 5px;" src="<%=imgLink%>" />
                                                        </div>
                                                  
                                                    <%}) %>
                
                
                                            </div>
                                        </div>
                                    </div><!-- col-lg-12 -->
                                </div><!-- End Row -->
                
                                <%} %>
                                




                <!-- Footer -->
                <footer class="footer text-right">
                    <div class="container">
                        <div class="row">
                            <div class="col-xs-6">
                                Â© 2022. All rights reserved.
                            </div>
                            <div class="col-xs-6">
                                <ul class="pull-right list-inline m-b-0">
                                    <li>
                                        <a target="_blank" href="http://pictureyourtravel.com/">PYT | Picture Your
                                            Travel</a>
                                    </li>


                                </ul>
                            </div>
                        </div>
                    </div>
                </footer>
                <!-- End Footer -->

            </div>
        </div>
    </div><!-- Wrapper -->



    <!--========== Location Info Ends Here ===================!-->

